@article{Viberg2004,
abstract = {The cochleogram is commonly used for illustrating hair cell loss after insult, yet standardized procedures for plotting either individual or averaged cochleograms are lacking despite more than 40 years of use. Due to the intra-species variation in basilar membrane (BM) length, it is important that length is plotted on the cochleogram in percent and not millimeter. It is also of interest to correlate the location of lesion to frequency by using a frequency-place equation. However, there is no consensus as which equation is most suitable for the species under study. This is an important issue since two different equations can result in significantly different frequency-place maps for the same cochlea. The purpose of this presentation is to suggest procedures for standardizing the cochleogram. The guidelines include: (i) basilar membrane length should be plotted as percent instead of millimeter due to the biological variation that exists in BM length within a particular species and strain, and the total length in millimeter stated on the cochleogram; (ii) the equations used for frequency-place maps should be stated on the cochleogram; (iii) different basilar membrane lengths should be normalized to percent before averaged cochleograms are made. These procedures are illustrated and discussed.},
author = {Viberg, Agneta and Canlon, Barbara},
doi = {10.1016/j.heares.2004.04.016},
file = {:home/manumouss/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Viberg, Canlon - 2004 - The guide to plotting a cochleogram.pdf:pdf},
isbn = {4685248724},
issn = {0378-5955},
journal = {Hearing research},
keywords = {Animals,Basilar Membrane,Basilar Membrane: anatomy \& histology,Basilar Membrane: physiology,Cochlea,Cochlea: anatomy \& histology,Cochlea: physiology,Evoked Potentials, Auditory, Brain Stem,Evoked Potentials, Auditory, Brain Stem: physiolog,Guinea Pigs,Hair Cells, Auditory,Hair Cells, Auditory: anatomy \& histology,Hair Cells, Auditory: pathology,Hair Cells, Auditory: physiology,Hearing Loss,Hearing Loss: physiopathology,Mice,Mice, Inbred CBA,Noise,Noise: adverse effects,Rats,Rats, Sprague-Dawley,Species Specificity},
month = nov,
